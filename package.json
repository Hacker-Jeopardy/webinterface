{
  "name": "jeopardy-webinterface",
  "private": true,
  "version": "1.0.0",
  "description": "Jeopardy Webinterface",
  "license": "MIT",
  "config": {
    "entry": "src/index.js",
    "ghooks": {
      "pre-commit": "npm test"
    },
    "html": {
      "title": "Jeopardy Webinterface",
      "description": "Jeopardy Webinterface",
      "author": "Simon Wörner"
    }
  },
  "scripts": {
    "build": "neo build --config webpack.prod.js",
    "start": "neo start --port 4000 --config webpack.dev.js",
    "lint": "node_modules/eslint/bin/eslint.js .",
    "lint:fix": "node_modules/eslint/bin/eslint.js . --fix",
    "test": "npm run lint && neo test --config karma.test.js",
    "test:watch": "neo test --config karma.test-watch.js --watch"
  },
  "keywords": [],
  "devDependencies": {
    "mozilla-neo": "^2.4.0"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.2",
    "ghooks": "1.3.2",
    "immutable": "3.8.1",
    "react": "15.3.0",
    "react-addons-test-utils": "15.3.0",
    "react-dom": "15.3.0",
    "react-redux": "4.4.5",
    "react-router": "2.6.1",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-devtools": "3.3.1",
    "redux-devtools-chart-monitor": "1.4.2",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-form": "^6.0.5",
    "redux-immutable": "3.0.6",
    "redux-logger": "2.6.1",
    "redux-slider-monitor": "1.0.7"
  },
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jeoparty/webinterface.git"
  },
  "author": "Simon Wörner",
  "bugs": {
    "url": "https://github.com/Jeoparty/webinterface/issues"
  },
  "homepage": "https://github.com/Jeoparty/webinterface#readme"
}
